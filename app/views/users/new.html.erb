<%= form_for(@user) do |f| %>
            <%= f.label "Name "%><br>
            <%=f.text_field :name %> <br>
            <%= f.label "Password "%><br>
            <%= f.password_field :password %> <br>
            <%= f.label "Number of Tickets "%><br>
             <%= f.number_field :tickets %> <br>
            <%= f.label "Height (in inches)"%><br>
             <%= f.number_field :height %> <br>
            <br>
            <%= f.label "How happy are you? 1-10" %> <br>
            <%= f.number_field :happiness, in: 1...11 %><br>
            <%= f.label "How nauseous are you? 1-10" %><br>
            <%= f.number_field :nausea, in: 1...11 %><br>
            <%= f.label "Are you an Admin?"%><br>
            <%= f.check_box :admin, id: "user_admin"%> <br>
			<%= f.submit %>
<% end %>
